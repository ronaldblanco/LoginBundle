{% extends "AcmeLoginBundle::datatable.html.twig" %}

{% block title %} {{app.session.get('login')}} | {{title}} | {% endblock %} 

{%block content%} 

<h1>Logins creation</h1>

<form action="{{ path('accesos_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('accesos') }}">
            Back to the list
        </a>
    </li>
</ul>

{%endblock%}
